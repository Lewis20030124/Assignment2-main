// Suffix Tree
digraph {
	Root [label=Root]
	"1 (0, 3)" [label="1 (0, 3)"]
	Root -> "1 (0, 3)" [label=abba]
	"U (1, 1)" [label="U (1, 1)"]
	Root -> "U (1, 1)" [label=b]
	"2 (2, 3)" [label="2 (2, 3)"]
	"U (1, 1)" -> "2 (2, 3)" [label=ba]
	"3 (3, 3)" [label="3 (3, 3)"]
	"U (1, 1)" -> "3 (3, 3)" [label=a]
}
