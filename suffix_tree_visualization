// Suffix Tree
digraph {
	Root [label=Root]
	"1 (0, 5)" [label="1 (0, 5)"]
	Root -> "1 (0, 5)" [label=banana]
	"3 (2, 5)" [label="3 (2, 5)"]
	"1 (0, 5)" -> "3 (2, 5)" [label=nana]
	"6 (4, 5)" [label="6 (4, 5)"]
	"1 (0, 5)" -> "6 (4, 5)" [label=na]
	"U (1, 2)" [label="U (1, 2)"]
	Root -> "U (1, 2)" [label=an]
	"2 (3, 5)" [label="2 (3, 5)"]
	"U (1, 2)" -> "2 (3, 5)" [label=ana]
	"4 (4, 4)" [label="4 (4, 4)"]
	"U (1, 2)" -> "4 (4, 4)" [label=n]
}
